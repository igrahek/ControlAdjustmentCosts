{
	"model":"modelMixed1",
	"study":"Mixed",
	"data":"DDM_Mixed_nSubs_44",
	"totalsample":14000,
	"burnedsample":12000,
	"predictors":{
		"intervalType":{
			"type":"category",
			"levels":["Speed","Accuracy"],
			"contrast":"Sum"
		},
		"congruency":{
			"type":"category",
			"levels":["incongruent","congruent"],
			"contrast":"Sum"
		},
		"blockType":{
			"type":"category",
			"levels":["Fixed","Mixed"],
			"contrast":"Sum"
		},
		"scaledRunningTime":{
			"type":"continuous"
		}
	},
	"formula":["a~intervalType*blockType+scaledRunningTime",
		   "v~intervalType*blockType+congruency",
		   "t~intervalType",
		   "z~congruency"
	],
	"bias":true,
	"includes":["p_outlier","sv","st"],
	"errors":"ae_only"
}